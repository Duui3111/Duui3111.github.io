RewriteEngine on 

RewriteCond %{REQUEST_FILENAME} !-d 
RewriteCond %{REQUEST_FILENAME}\.html -f  

RewriteRule ^(.*)$ $1.html [NC,L] 

RewriteRule ^(.*)$.html /$1 [L]
Redirect ^(.*)$.html /$1 [L]
Redirect /testt.html Home.html

ErrorDocument 404 /404.html

RewriteEngine On
RewriteRule ^pages/(.*)$ /$1 [L]


